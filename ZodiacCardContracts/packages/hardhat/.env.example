# Block Explorer API Keys
CELOSCAN_API_KEY=
ETHERSCAN_API_KEY=

# Deployment Configuration
VERIFY_ON_DEPLOY=true

# Celo RPC URLs (optional - defaults available)
CELO_MAINNET_RPC_URL=https://forno.celo.org
CELO_ALFAJORES_RPC_URL=https://alfajores-forno.celo-testnet.org

# Contract Addresses (filled after deployment)
PROXY_CONTRACT_ADDRESS=
IMPLEMENTATION_CONTRACT_ADDRESS=

# Deployment Addresses (REQUIRED for deployment)
DEPLOYER_ADDRESS=
TREASURY_ADDRESS=

# Minting Configuration
CELO_MINT_FEE=10.0 # Fee in CELO (18 decimals, e.g., 10.0 = 10 CELO)

# Private Key Configuration (REQUIRED - set via script or manually)
# DO NOT commit your actual private key to version control
# Use: yarn account:import to securely set this
__RUNTIME_DEPLOYER_PRIVATE_KEY=

